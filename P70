#include<stdio.h>
int main()
{
    int vivo=20000,oneplus=45000,oppo=12000,iphone=100000,mi=66000,min,max;
    printf("enter the value of min  and max\n");
    scanf("%d%d",&min,&max);
    if((oppo>=min)&&(oppo<=max))
    {
        printf("the available brand in budget range is oppo\n");
    }
    if((vivo>=min)&&(vivo<=max))
    {
        printf("the available brand in budget range is vivo\n");
    }
    if((oneplus>=min)&&(oneplus<=max))
    {
        printf("the brand name is oneplus\n");
    }
    if((mi>=min)&&(mi<=max))
    {
        printf("the brand name is mi\n");
    }
    if((iphone>=min)&&(iphone<=max))
    {
        printf("the brand name is iphone\n");
    }
    return 0;
}
