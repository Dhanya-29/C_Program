#include <stdio.h>
int main() {
    char m1[]="redmi";
    char m2[]="oppo";
    char m3[]="vivo";
    char m4[]="Samsung";
    char m5[]="apple";
    int p1=10000;
    int p2=25000;
    int p3=40000;
    int p4=70000;
    int p5=100000;
    int bud_min, bud_max;
    printf("Enter a minimum budget range:");
    scanf("%d",& bud_min);
    printf("Enter a maximum budget range:");
    scanf("%d",& bud_max);
    if(p1>=bud_min&& p1<=bud_max)
    {
        printf("%s",m1);
    }
    else if(p2>=bud_min&&p2<=bud_max){
        printf("%s",m2);
    }
    else if(p3>=bud_min&& p3<=bud_max){
        printf("%s",m3);
    }
    else if(p4>=bud_min&&p4<=bud_max){
        printf("%s",m4);
    }
    else if(p5>=bud_min && p5<=bud_max){
        printf("%s",m5);
    }
    return 0;
}
