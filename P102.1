int duplicate_ele(int arr[10],int n){
    int i,j,count=0;
    for(i=0;i<n;i++)
    {
      for(j=i+1;j<n;j++)
      {
        if(arr[i]==arr[j])
         {
            count++;
 
          }
               
       }
    
     }
  return count;
}
