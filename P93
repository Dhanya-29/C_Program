#include<stdio.h>
long int factorial(int n);
int main() {
    int n;
    printf("Enter a positive integer: ");
    scanf("%d",&n);
    long int factorial( int n);
    printf("Factorial of %d = %ld", n,factorial(n));
    return 0;
}

long int factorial(int num) {
    if (num>=1)
        return num*factorial(num-1);
    else
        return 1;
}
