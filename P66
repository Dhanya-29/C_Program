#include<stdio.h>
int main(){
    char c;
    printf("enter a character:");
    scanf("%c",&c);
    if((c>='a'&&c<='z')||(c>='A'&&c<='Z')){
        printf("it is a alphabet",c);
    }
    else if((c>='0'&&c<='9')){
        printf("it is a digit");
    }
    else
    printf("it is a special character");
}
