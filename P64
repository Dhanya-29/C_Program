#include<stdio.h>
int main(){
    int a,b,c;
    printf("enter the values of a b and c:");
    scanf("%d %d %d",&a,&b,&c);
    if((a==b)&&(b==c)&&(c==a)){
        printf("it is equilateral triangle");
    }
    else if(((a==b)&&(b!=c)&&(c!=a))||((a!=b)&&(b!=c)&&(c==a))||((a!=b)&&(b==c)&&(c!=a))){
        printf("it is a isosceles triangle");
    }
    else
    printf("its scalar triangle");
}
